{
  "name": "symbols",
  "version": "0.1.0",
  "private": true,
  "homepage": "/symbols",
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "@types/classnames": "^2.2.11",
